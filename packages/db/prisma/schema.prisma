datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

model Weight {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  date      DateTime
  note      String?
  updatedAt DateTime @updatedAt
  weight    Int
}
